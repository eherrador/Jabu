@model IEnumerable<WebAppMvcJabu.Models.RegisterViewModel>
<header>
    <div style="width:100%; padding:10px; margin-top:20px; margin-bottom:10px; background-color:#1396e2; color:#ffffff;">
        Usuarios
    </div>
</header>
<section>
    <table class="table">
        <tr>
            <th>
                Usuario
            </th>
            <th>
                Rol
            </th>
            <th>

            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @foreach (var rol in item.RolesList)
                    {
                        if (rol.Selected) { @rol.Text; }
                    }
                </td>
                <td>
                    <a href="javascript:fn_EditarUsuario('@item.Id');">Editar</a>&nbsp;|&nbsp;
                    <a href="javascript:fn_EliminarUsuario('@item.Id', '@item.Email');">Eliminar</a>
                </td>
            </tr>
        }

    </table>
</section>